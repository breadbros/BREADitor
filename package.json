{
  "name": "Breaditor",
  "productName": "Breaditor",
  "description": "A 2d game map editor, made by Breadbros. Games",
  "version": "2.0.1",
  "author": "Ben 'Grue' McGraw <ben.mcgraw@gmail.com>",
  "copyright": "Â© 2014, Breadbros Games",
  "homepage": "http://breaditor.com",
  "license": "MIT",
  "main": "app/background.js",
  "repository": "https://github.com/mcgrue/BREADitor",
  "build": {
    "appId": "com.example.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "jest",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register",
    "lint": "eslint .",
    "validate": "npm ls"
  },
  "dependencies": {
    "electron-canvas-to-buffer": "^2.0.0",
    "fs-jetpack": "^0.10.2",
    "lodash": "^4.17.2",
    "sprintf-js": "^1.0.3"
  },
  "devDependencies": {
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-jest": "^17.0.2",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "electron": "^1.4.10",
    "electron-builder": "^8.6.0",
    "electron-mocha": "^3.0.0",
    "eslint-plugin-react": "^7.9.1",
    "gulp": "^3.9.0",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.3",
    "jest": "^17.0.3",
    "jest-cli": "^17.0.3",
    "jquery": "^3.1.1",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "precommit-hook-eslint": "^3.0.0",
    "rollup": "^0.36.3",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.2",
    "spectron": "^3.3.0"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}
