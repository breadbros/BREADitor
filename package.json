{
  "name": "breaditor",
  "version": "2.0.1",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env NODE_ENV=test START_HOT=1 npm run start-renderer-dev",
    "dev": "npm run start-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "eslint --ext=jsx,js,tsx,ts src",
    "lint-fix": "eslint --fix --ext=jsx,js,tsx,ts src",
    "test": "jest --detectOpenHandles",
    "watch": "jest --detectOpenHandles --watchAll",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest --testMatch '**/?(*.)+(e2etest).[tj]s?(x)'",
    "test-circle": "npm test && npm run test:e2e",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{json,css,scss,md}": [
      "prettier --config ./.prettierrc --write"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "eslint --ext=jsx,js,ts,tsx --fix src"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    },
    "setupFiles": [
      "./setup-jest.js"
    ]
  },
  "build": {
    "productName": "Breaditor",
    "appId": "com.breadbros.breaditor",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Robinfr/electron-react-typescript.git"
  },
  "author": {
    "name": "Ben McGraw",
    "email": "ben@breadbros.com"
  },
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/mcgrue/breaditor/issues"
  },
  "homepage": "https://breaditor.com",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^29.1.0",
    "@types/jquery": "^3.5.14",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/source-map-support": "^0.5.6",
    "@types/webdriverio": "^5.0.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@webpack-cli/serve": "^1.7.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "electron": "^21.0.1",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^29.1.1",
    "react-refresh": "^0.14.0",
    "redux-devtools-extension": "^2.13.9",
    "source-map-loader": "^4.0.0",
    "spectron": "^19.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@sphinxxxx/color-conversion": "^2.2.2",
    "@testing-library/react": "^13.4.0",
    "acorn": "^8.8.0",
    "css-loader": "^6.7.1",
    "electron-canvas-to-buffer": "^2.0.0",
    "electron-context-menu": "^3.5.0",
    "events": "^3.3.0",
    "file-loader": "^6.2.0",
    "fs-jetpack": "^5.0.0",
    "is-electron": "^2.2.1",
    "jquery": "^3.6.1",
    "jquery-contextmenu": "^2.9.2",
    "jquery-ui": "^1.13.2",
    "jquery-ui-dist": "^1.13.2",
    "lodash": "^4.17.21",
    "natives": "^1.1.6",
    "pngjs": "^6.0.0",
    "react": "^18.2.0",
    "react-dockable": "git@github.com:breadbros/react-dockable.git",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "source-map-support": "^0.5.21",
    "spectrum-colorpicker": "^1.8.1",
    "sprintf-js": "^1.1.2",
    "style-loader": "^3.3.1",
    "typescript-plugin-css-modules": "^5.0.1",
    "ui-contextmenu": "^1.18.1",
    "uuid": "^9.0.0"
  }
}
